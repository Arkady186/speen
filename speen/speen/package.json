{
  "name": "speen-miniapp-monorepo",
  "private": true,
  "workspaces": [
    "apps/server",
    "miniapp"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:server": "npm --workspace apps/server run dev",
    "build": "npm --workspace miniapp run build && npm --workspace apps/server run build",
    "start": "npm --workspace apps/server start"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}

